{
  "name": "back-office",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "NODE_ICU_DATA=\"$(pwd)/node_modules/full-icu\" npm run -s test:unit",
    "lint": "run-p lint:*",
    "build:admin": "vue-cli-service build",
    "build:candidat": "vue-cli-service build",
    "format": "run-s lint:js",
    "lint:js": "vue-cli-service lint",
    "lint:style": "vue-cli-service lint:style",
    "prebuild:admin": "cp .env.admin .env",
    "prebuild:candidat": "cp .env.candidat .env",
    "start": "npm run serve",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "sb": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "test:e2e": "vue-cli-service test:e2e",
    "cypress": "cypress run",
    "test:unit": "vue-cli-service test:unit",
    "test:watch": "NODE_ICU_DATA=\"$(pwd)/node_modules/full-icu\" npm run -s test:unit -- --watch"
  },
  "dependencies": {
    "ag-grid-community": "^21.1.1",
    "ag-grid-vue": "^21.1.0",
    "cypress-file-upload": "^3.3.3",
    "cypress-mailhog": "^1.1.0",
    "delay": "^4.3.0",
    "html-loader": "^0.5.5",
    "intl": "^1.2.5",
    "luxon": "^1.17.2",
    "normalize.css": "^8.0.1",
    "p-debounce": "^2.1.0",
    "register-service-worker": "^1.6.2",
    "v-clipboard": "^2.2.2",
    "vue": "^2.6.10",
    "vue-carousel": "^0.18.0",
    "vue-intl": "^3.1.0",
    "vue-property-decorator": "^8.2.1",
    "vue-router": "^3.1.2",
    "vue-scrollto": "^2.15.0",
    "vuetify": "^1.5.16",
    "vuex": "^3.1.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.2",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-notes": "^5.1.11",
    "@storybook/addon-viewport": "^5.1.11",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-e2e-cypress": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-pwa": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.2.3",
    "full-icu": "^1.3.0",
    "material-design-icons-iconfont": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-current-selector": "0.0.3",
    "postcss-nested": "^4.1.2",
    "postcss-nested-ancestors": "^2.0.0",
    "postcss-nesting": "^7.0.1",
    "postcss-preset-env": "^6.7.0",
    "resolve-url-loader": "^3.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "svg-inline-loader": "^0.8.0",
    "typeface-roboto": "0.0.75",
    "vue-cli-plugin-storybook": "^0.6.1",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-double"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-preset-env": {},
      "postcss-nested-ancestors": {},
      "postcss-nested": {},
      "postcss-current-selector": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/src/**/*.spec.js|**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
